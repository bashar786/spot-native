{
  "expo": {
    "name": "Spot",
    "slug": "Spot",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/spotnew.jpeg",
      "backgroundColor": "#1E3B2F",
      "resizeMode": "contain",
      "imageResizeMode": "contain",
      "android": {
        "backgroundColor": "#1E3B2F",
        "resizeMode": "cover",
        "xxhdpi": "./assets/images/spotnew.jpeg",
        "xxxhdpi": "./assets/images/spotnew.jpeg"
      },
      "ios": {
        "backgroundColor": "#1E3B2F",
        "resizeMode": "contain",
        "xxhdpi": "./assets/images/spotnew.jpeg",
        "xxxhdpi": "./assets/images/spotnew.jpeg"
      }
    },
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "scheme": "Spot",
      "bundleIdentifier": "com.spotpay.network"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spotnew.jpeg",
        "backgroundColor": "#1E3B2F"
      },
      "scheme": "Spot",
      "package": "com.spotpay.network"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
